#include <string>
#include <vector>

using namespace std;

int solution(vector<int> numbers, int target) {
    int answer = 0;
    vector<int> cur;
    cur.push_back(0);
    for(int i : numbers){
        vector<int> tmp;
        for(int j : cur){
            tmp.push_back(j - i);
            tmp.push_back(j + i);
        }
        cur = tmp;
    }
    for(int i : cur){
        if(i == target){
            answer++;
        }
    }
    return answer;
}
